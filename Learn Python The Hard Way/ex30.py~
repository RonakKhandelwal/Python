people=30
cars=40
buses=15

if cars >people:
    print"We shold take cars.."

elif cars <people:
    print"We should not take cars"    

else:
    print "We can't decide"    

if buses>cars:
    print"Too many buses"    
    
elif busses <cars:
    print"Maybe we could take buses"

else:
    print"We still can't decide"

if people>buses:
    print"Alright, lets just take buses"    

else:
    print"Fine,lets stay home then"
